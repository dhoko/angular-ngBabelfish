  "devDependencies": {
    "gulp": "^3.8.11",
    "gulp-beautify": "^1.1.2",
    "gulp-concat": "^2.5.2",
    "gulp-ng-annotate": "^0.5.2",
    "gulp-plumber": "^0.6.6",
    "gulp-sourcemaps": "^1.5.0",
    "gulp-tap": "^0.1.3",
    "gulp-uglify": "^1.1.0",
